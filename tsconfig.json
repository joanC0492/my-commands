{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*.js", "src/**/*.ts", "src/**/*.json"],
  "exclude": ["node_modules", "pruebas", "src/templates/static-files"]
}
